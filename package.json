{
  "name": "gr-tools",
  "version": "9.0.1",
  "description": "Commands to make my life easier",
  "main": "src/index.js",
  "engines": {
    "node": ">=12"
  },
  "os": [
    "linux"
  ],
  "scripts": {
    "dev": "npm uninstall -g gr-tools && npm link",
    "lint": "standard",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "node ./src/scripts/postinstall.js",
    "reinstall": "npx rimraf node_modules && npm ci",
    "test": "jest",
    "test:cov": "npm run test -- --coverage"
  },
  "bin": {
    "gr-tools": "src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielrufino/gr-tools.git"
  },
  "keywords": [
    "CLI",
    "tools"
  ],
  "author": "Gabriel Rufino <contato@gabrierufino.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielrufino/gr-tools/issues"
  },
  "homepage": "https://github.com/gabrielrufino/gr-tools#readme",
  "dependencies": {
    "@gabrielrufino/is-required": "^1.0.14",
    "axios": "^0.21.4",
    "commander": "^6.2.1",
    "compare-versions": "^3.6.0",
    "figlet": "^1.7.0",
    "folquire": "^1.0.29",
    "inquirer": "^7.3.3",
    "latest-version": "^5.1.0",
    "omelette": "^0.4.17",
    "ora": "^5.4.1",
    "shelljs": "^0.8.5",
    "systeminformation": "^5.21.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "git-branch-is": "^4.0.0",
    "jest": "^27.5.1",
    "rimraf": "^5.0.5",
    "standard": "^16.0.4"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
